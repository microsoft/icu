    "featureFilters": {
        "conversion_mappings": "exclude",
        "confusables": "exclude",
        "stringprep": "exclude",
        "zone_tree": "exclude",
        "zone_supplemental": "exclude",
        "translit": "exclude",
        "unames": "exclude",
        "ulayout": "exclude",
        "unit_tree": "exclude",
        "rbnf_tree": "exclude",
        "cnvalias": "exclude",
        "lang_tree": "exclude",
        "region_tree": "exclude",
        "misc": {
            "whitelist": [
                "currencyNumericCodes",
                "numberingSystems",
                "icuver",
                "langInfo",
                "supplementalData",
                "keyTypeData"
            ]
        },
        "brkitr_dictionaries": "exclude",
        "brkitr_rules": {
            "whitelist": ["char"]
        }
    },
    "resourceFilters": [
        {
            "categories": ["curr_tree"],
            "rules": [
                "-/Currencies",
                "+/Currencies/SAR",
                "+/Currencies/BGN",
                "+/Currencies/BDT",
                "+/Currencies/INR",
                "+/Currencies/EUR",
                "+/Currencies/CZK",
                "+/Currencies/DKK",
                "+/Currencies/GBP",
                "+/Currencies/USD",
                "+/Currencies/IRR",
                "+/Currencies/PHP",
                "+/Currencies/ILS",
                "+/Currencies/HRK",
                "+/Currencies/HUF",
                "+/Currencies/IDR",
                "+/Currencies/JPY",
                "+/Currencies/KRW",
                "+/Currencies/MYR",
                "+/Currencies/PLN",
                "+/Currencies/BRL",
                "+/Currencies/RON",
                "+/Currencies/RUB",
                "+/Currencies/RSD",
                "+/Currencies/SEK",
                "+/Currencies/CDF",
                "+/Currencies/KES",
                "+/Currencies/TZS",
                "+/Currencies/UGX",
                "+/Currencies/LKR",
                "+/Currencies/SGD",
                "+/Currencies/THB",
                "+/Currencies/TRY",
                "+/Currencies/UAH",
                "+/Currencies/VND",
                "+/Currencies/CNY",
                "+/Currencies/TWD",
                "-/Currencies%narrow",
                "-/Currencies%formal",
                "-/Currencies%variant",
                "-/CurrencyPlurals",
                "-/CurrencyUnitPatterns"
            ]
        },
        {
            "categories": ["locales_tree"],
            "rules": [
                "-/characterLabel",
                "-/measurementSystemNames",
                "-/listPattern",
                "-/fields",
                "-/delimiters",
                "-/Ellipsis",
                "-/NumberElements/latn/miscPatterns",
                "-/NumberElements/latn/patternsLong",
                "-/NumberElements/latn/patternsShort",
                "-/NumberElements/*/patternsLong",
                "-/NumberElements/*/patternsShort",
                "-/NumberElements/minimalPairs",
                "-/parse",
                "-/AuxExemplarCharacters",
                "-/ExemplarCharacters",
                "-/ExemplarCharactersIndex",
                "-/ExemplarCharactersNumbers",
                "-/ExemplarCharactersPunctuation",
                "-/MoreInformation"
            ]
        },
        {
            "categories": ["locales_tree"],
            "files": {
                "blacklist": [
                    "root",
                    "am",
                    "ar",
                    "fa",
                    "he",
                    "hi",
                    "ja",
                    "ko",
                    "th",
                    "zh",
                    "zh_Hant"
                ]
            },
            "rules": [
                "-/calendar/*",
                "+/calendar/default",
                "+/calendar/gregorian",
                "+/calendar/generic"
            ]
        },
        {
            "categories": ["coll_tree"],
            "rules": [
                "-/*/*",
                "+/collations/default",
                "+/collations/standard",
                "+/collations/private-kana",
                "-/UCARules"
            ]
        },
        {
            "categories": ["misc"],
            "files": {
                "whitelist": ["supplementalData"]
            },
            "rules": [
                "-/*",
                "+/calendarData",
                "+/calendarPreferenceData",
                "+/cldrVersion",
                "+/measurementData",
                "+/codeMappings",
                "+/idValidity",
                "+/timeData",
                "+/weekData",
                "+/weekData%variant"
            ]
        }
    ]
}
